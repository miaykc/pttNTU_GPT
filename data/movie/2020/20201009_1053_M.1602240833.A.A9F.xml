   <text>
      <body author="artpoet (靈魂之舞)">
         <s>
            <w type="Nb">魏</w>
            <w type="DE">的</w>
            <w type="Na">片</w>
            <w type="VJ">受限於</w>
            <w type="Na">題材</w>
            <w type="Na">內容</w>
            <w type="COMMACATEGORY">，</w>
            <w type="VK">注定</w>
            <w type="Da">只有</w>
            <w type="Nc">台灣</w>
            <w type="Nc">市場</w>
            <w type="VA">買單</w>
         </s>
         <s>
            <w type="Cbb">然而</w>
            <w type="Nc">台灣</w>
            <w type="Nc">市場</w>
            <w type="D">又</w>
            <w type="Dfa">太</w>
            <w type="VH">小</w>
            <w type="COMMACATEGORY">，</w>
            <w type="P">以</w>
            <w type="VH">這樣</w>
            <w type="A">非商業性</w>
            <w type="DE">的</w>
            <w type="Na">題材</w>
         </s>
         <s>
            <w type="VK">注定</w>
            <w type="VC">拍出來</w>
            <w type="Dfa">很</w>
            <w type="VH">難</w>
            <w type="VC">賺到</w>
            <w type="VL">足以</w>
            <w type="VL">讓</w>
            <w type="Nc">台灣</w>
            <w type="Na">三部曲</w>
            <w type="VA">回本</w>
            <w type="DE">的</w>
            <w type="Na">錢</w>
         </s>
         <s>
            <w type="Cbb">然而</w>
            <w type="COMMACATEGORY">，</w>
            <w type="Nb">魏</w>
            <w type="VE">想</w>
            <w type="VC">拍</w>
            <w type="DE">的</w>
            <w type="Na">片</w>
            <w type="COMMACATEGORY">，</w>
            <w type="Cbb">雖然</w>
            <w type="VJ">缺乏</w>
            <w type="Na">商業</w>
            <w type="Na">價值</w>
            <w type="COMMACATEGORY">，</w>
            <w type="Cbb">但</w>
            <w type="D">卻</w>
            <w type="VJ">充滿</w>
            <w type="Di">了</w>
            <w type="VH">滿滿</w>
            <w type="DE">的</w>
            <w type="PARENTHESISCATEGORY">「</w>
            <w type="Nc">台灣</w>
            <w type="Na">價值</w>
            <w type="PARENTHESISCATEGORY">」</w>
         </s>
         <s>
            <w type="Nep">其</w>
            <w type="Nc">台灣</w>
            <w type="Na">價值</w>
            <w type="DE">的</w>
            <w type="Na">濃度</w>
            <w type="COMMACATEGORY">，</w>
            <w type="D">可</w>
            <w type="VE">說</w>
            <w type="SHI">是</w>
            <w type="VC">超越</w>
            <w type="Nd">當前</w>
            <w type="Nc">檯面</w>
            <w type="Ncd">上</w>
            <w type="DE">的</w>
            <w type="Neqa">所有</w>
            <w type="Nc">台灣</w>
            <w type="Na">導演</w>
         </s>
         <s>
            <w type="D">就</w>
            <w type="P">像</w>
            <w type="Nh">你</w>
            <w type="D">所</w>
            <w type="VE">說</w>
            <w type="T">的</w>
         </s>
         <s>
            <w type="Cbb">連</w>
            <w type="Nc">國際</w>
            <w type="Na">大師</w>
            <w type="Nb">黑澤明</w>
            <w type="D">都</w>
            <w type="D">必須</w>
            <w type="P">跟</w>
            <w type="Nc">美國</w>
            <w type="VB">求援</w>
            <w type="COMMACATEGORY">，</w>
            <w type="VI">沒辦法</w>
            <w type="VE">做到</w>
            <w type="P">像</w>
            <w type="Nh">你</w>
            <w type="VE">說</w>
            <w type="DE">的</w>
            <w type="P">用</w>
            <w type="Nh">自己</w>
            <w type="VC">賺</w>
            <w type="DE">的</w>
            <w type="Na">錢</w>
            <w type="D">來</w>
            <w type="VC">拍</w>
         </s>
         <s>
            <w type="Na">連</w>
            <w type="Na">動畫</w>
            <w type="VH">大神</w>
            <w type="Nb">押井守</w>
            <w type="COMMACATEGORY">，</w>
            <w type="D">也</w>
            <w type="D">不</w>
            <w type="SHI">是</w>
            <w type="P">用</w>
            <w type="Nh">自己</w>
            <w type="VC">賺</w>
            <w type="DE">的</w>
            <w type="Na">錢</w>
            <w type="D">來</w>
            <w type="VC">拍</w>
            <w type="COMMACATEGORY">，</w>
            <w type="Cbb">而是</w>
            <w type="VC">燒</w>
            <w type="Nh">別人</w>
            <w type="DE">的</w>
            <w type="Na">錢</w>
            <w type="D">來</w>
            <w type="VC">拍</w>
         </s>
         <s>
            <w type="PARENTHESISCATEGORY">（</w>
            <w type="Cbb">雖然</w>
            <w type="Nh">我</w>
            <w type="D">不</w>
            <w type="Dfa">太</w>
            <w type="VK">懂</w>
            <w type="P">跟</w>
            <w type="Nb">史匹柏</w>
            <w type="VC">要</w>
            <w type="Na">錢</w>
            <w type="COMMACATEGORY">，</w>
            <w type="P">與</w>
            <w type="P">跟</w>
            <w type="Nb">郭台銘</w>
            <w type="VC">要</w>
            <w type="Na">錢</w>
            <w type="V_2">有</w>
            <w type="Nep">什麼</w>
            <w type="Na">差別</w>
            <w type="VH">就是了</w>
            <w type="PERIODCATEGORY">...</w>
            <w type="PARENTHESISCATEGORY">）</w>
         </s>
         <s>
            <w type="Cbb">而</w>
            <w type="Nb">北野武</w>
            <w type="Cbb">雖然</w>
            <w type="SHI">是</w>
            <w type="P">用</w>
            <w type="Nh">自己</w>
            <w type="DE">的</w>
            <w type="Na">錢</w>
            <w type="D">來</w>
            <w type="VC">拍</w>
            <w type="COMMACATEGORY">，</w>
            <w type="Cbb">但</w>
            <w type="Nh">他</w>
            <w type="VE">想</w>
            <w type="VC">拍</w>
            <w type="DE">的</w>
            <w type="Na">內容</w>
            <w type="Dfa">遠遠</w>
            <w type="D">不</w>
            <w type="VK">需要</w>
            <w type="Nep">什麼</w>
            <w type="Neu">四十億</w>
         </s>
         <s>
            <w type="Dk">那麼</w>
            <w type="VG">做為</w>
            <w type="Nc">台灣</w>
            <w type="Na">大師</w>
            <w type="DE">的</w>
            <w type="Nb">魏</w>
            <w type="COMMACATEGORY">，</w>
            <w type="D">沒有</w>
            <w type="P">跟</w>
            <w type="Nc">美國</w>
            <w type="VB">求援</w>
            <w type="DE">的</w>
            <w type="Na">基礎</w>
         </s>
         <s>
            <w type="D">不</w>
            <w type="P">像</w>
            <w type="Nb">押井守</w>
            <w type="VH">那樣</w>
            <w type="V_2">有</w>
            <w type="Na">人</w>
            <w type="VK">願意</w>
            <w type="VA">燒錢</w>
            <w type="P">給</w>
            <w type="Nh">他</w>
            <w type="VD">賠</w>
         </s>
         <s>
            <w type="Cbb">但</w>
            <w type="D">又</w>
            <w type="V_2">有</w>
            <w type="Di">著</w>
            <w type="VJ">高達</w>
            <w type="Neu">四十億</w>
            <w type="DE">的</w>
            <w type="Nc">台灣</w>
            <w type="Na">三部曲</w>
            <w type="DE">的</w>
            <w type="Na">夢想</w>
         </s>
         <s>
            <w type="Dk">那麼</w>
            <w type="COMMACATEGORY">，</w>
            <w type="P">用</w>
            <w type="Na">片</w>
            <w type="Ng">裡</w>
            <w type="DE">的</w>
            <w type="Nc">台灣</w>
            <w type="Na">價值</w>
            <w type="D">來</w>
            <w type="P">跟</w>
            <w type="Na">台灣人</w>
            <w type="VB">求援</w>
            <w type="COMMACATEGORY">，</w>
            <w type="D">可</w>
            <w type="VE">說</w>
            <w type="SHI">是</w>
            <w type="VH">適得適所</w>
         </s>
         <s>
            <w type="D">再</w>
            <w type="VH">正確</w>
            <w type="Dfb">不過</w>
            <w type="T">了</w>
         </s>
         <s>
            <w type="Cbb">不然</w>
            <w type="COMMACATEGORY">，</w>
            <w type="Cbb">連</w>
            <w type="Nb">復聯四</w>
            <w type="D">都</w>
            <w type="Da">只</w>
            <w type="D">能</w>
            <w type="Na">營收</w>
            <w type="VJ">不</w>
            <w type="P">到</w>
            <w type="Neu">10億</w>
            <w type="DE">的</w>
            <w type="Nc">台灣</w>
            <w type="Nc">市場</w>
         </s>
         <s>
            <w type="Nh">你</w>
            <w type="VE">要</w>
            <w type="Nb">魏</w>
            <w type="D">怎麼</w>
            <w type="VC">賺到</w>
            <w type="Neu">40億</w>
            <w type="COMMACATEGORY">，</w>
            <w type="D">來</w>
            <w type="VC">達成</w>
            <w type="Nh">你</w>
            <w type="Na">口</w>
            <w type="Ncd">中</w>
            <w type="PARENTHESISCATEGORY">「</w>
            <w type="P">用</w>
            <w type="Nh">自己</w>
            <w type="VC">賺</w>
            <w type="DE">的</w>
            <w type="Na">錢</w>
            <w type="D">來</w>
            <w type="VA">拍片</w>
            <w type="PARENTHESISCATEGORY">」</w>
            <w type="QUESTIONCATEGORY">？</w>
         </s>
         <s>
            <w type="Caa">還是</w>
            <w type="Nh">你</w>
            <w type="VK">覺得</w>
            <w type="COMMACATEGORY">，</w>
            <w type="Nb">魏</w>
            <w type="D">應該</w>
            <w type="VE">認定</w>
            <w type="Nh">自己</w>
            <w type="D">不</w>
            <w type="VH">夠</w>
            <w type="Na">資格</w>
            <w type="VJ">擁有</w>
            <w type="Nep">這</w>
            <w type="Nf">個</w>
            <w type="Na">夢想</w>
         </s>
         <s>
            <w type="VH">直接</w>
            <w type="VC">放棄</w>
            <w type="VC">拍</w>
            <w type="Nc">台灣</w>
            <w type="Na">三部曲</w>
            <w type="T">呢</w>
            <w type="QUESTIONCATEGORY">？</w>
         </s>
      </body>
